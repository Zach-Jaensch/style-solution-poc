{
  "extends": ["@internal/typescript-config"],
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "plugins": [{ "name": "next" }],
    "target": "ES2017",
    "paths": {
      "#/*": ["./src/*"]
    }
  },
  "include": [
    ".next/types/**/*.ts",
    "*.*js",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts"
  ],
  "exclude": ["node_modules"]
}
