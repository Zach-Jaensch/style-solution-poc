{
  "name": "frontend-unified-library",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "baseline:dev": "turbo watch dev --filter=baseline-nextjs",
    "overriding:dev": "turbo watch dev --filter=overriding-nextjs",
    "prettier": "prettier -c .",
    "tailwind:dev": "turbo watch dev --filter=tailwindcss-nextjs",
    "typecheck": "turbo typecheck --continue",
    "vanilla-extract:dev": "turbo watch dev --filter=vanilla-extract-nextjs"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-packagejson": "^2.5.1",
    "turbo": "^2.0.9",
    "yarnhook": "^0.6.2"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": "20.x"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.3",
      "@types/react-dom": "18.3.0"
    }
  }
}
